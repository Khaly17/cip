  <log4net>
    <!-- Define some output appenders -->
    <!-- RollingFileAppender looks after rolling over files by size or date -->
    <appender name="RollingLogFileAppender" type="log4net.Appender.RollingFileAppender">
      <file value="${ALLUSERSPROFILE}\\Gefco\\CipQuai.Web" />
      <appendToFile value="true" />
      <rollingStyle value="Date" />
      <datePattern value="'-'yyyyMMdd'.log'" />
      <param name="StaticLogFileName" value="false" />
      <layout type="log4net.Layout.PatternLayout">
        <param name="Header" value="[Header]--------------------------------------[Header]&#xA;" />
        <param name="Footer" value="[Footer]--------------------------------------[Footer]&#xA;&#xA;                     * * *&#xA;" />
        <conversionPattern value="%date [%thread] %-5level %logger [%property{NDC}] - %message%newline" />
      </layout>
    </appender>
    <!-- Setup the root category, add the appenders and set the default level -->
    
    <root>
      <level value="DEBUG" />
      <appender-ref ref="RollingLogFileAppender" />
      <appender-ref ref="aiAppender" />
    </root>
    <!-- I-DISPO category - the default for I-DISPO -->
    <logger name="Gefco">
      <level value="DEBUG" />
    </logger>
    <!-- work in progress category -->
    <logger name="wip">
      <level value="DEBUG" />
    </logger>
    <appender name="aiAppender" type="Microsoft.ApplicationInsights.Log4NetAppender.ApplicationInsightsAppender, Microsoft.ApplicationInsights.Log4NetAppender">
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%message%newline" />
      </layout>
    </appender>
  </log4net>
